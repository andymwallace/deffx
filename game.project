[project]
title = DefFX
version = 1.0
dependencies = 

[input]
game_binding = /input/game.input_bindingc

[bootstrap]
main_collection = /examples_postprocessing/blur_simple/blur_simple.collectionc
render = /deffx/render/default.renderc

[display]
width = 1600
height = 800

